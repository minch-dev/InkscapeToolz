    <id>filter.inkscape.update.defaultstyle</id>
    <dependency type="executable" location="extensions">gz_update_dafault_style.py</dependency>
    <dependency type="executable" location="extensions">inkex.py</dependency>
    <effect>
        <object-type>all</object-type>
        <effects-menu>
